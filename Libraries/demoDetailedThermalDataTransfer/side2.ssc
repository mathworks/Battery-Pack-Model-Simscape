component side2
    % Copyright 2020-2024 The MathWorks, Inc.

    nodes
        a = foundation.thermal.thermal; % a:right
    end
    outputs
        side2Data={zeros(2,1),'1'}; % Q:left
    end
    variables
        q = {0, 'W'}
    end
    branches
        q : * -> a.Q;
    end
    equations
        a.T == {side2Data(1,1),'K'};
        q   == {side2Data(2,1),'W'};
    end
end 